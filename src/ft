
#include <stdio.h>

#define APPSEC_MAX  15

typedef struct appsec_feature_t {
    const char *who;
    const char *appsec[APPSEC_MAX+1];
} appsec_feature_t;

static appsec_feature_t appsec_features[] =
{
    {"htp",  {"1", "2", "3"}},
    {"smtp", {"a", "b"}},
    {"pop3", {"_1", "_2", "_3", "_4", "_5", "_6"}},
    {NULL,   {NULL,}},
};

void check_appsec_feature()
{
    int i = 0, k = 0;
    const char **appsec;

    while (appsec_features[i].who) {
        printf("%s\n", appsec_features[i].who);
        appsec = appsec_features[i].appsec;
        while (*appsec) {
            printf("\t%s\n", *appsec);
            appsec++;
        }

        i++;
    }
}


int main()
{
    check_appsec_feature();
    return 0;
}
